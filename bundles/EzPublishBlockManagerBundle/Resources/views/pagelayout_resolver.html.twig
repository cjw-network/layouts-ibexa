{% set main_pagelayout = viewbaseLayout is defined ? viewbaseLayout : 'NetgenBlockManagerBundle::pagelayout_empty.html.twig' %}

{% if noLayout is not defined or not noLayout %}
    {% if ngbm.configuration.hasParameter('pagelayout') %}
        {% set main_pagelayout = ngbm.configuration.getParameter('pagelayout') %}
    {% elseif pagelayout is defined %}
        {% set main_pagelayout = pagelayout %}
    {% elseif ezpublish.configResolver.hasParameter('pagelayout') %}
        {% set main_pagelayout = ezpublish.configResolver.parameter('pagelayout') %}
    {% endif %}
{% endif %}

{% extends main_pagelayout %}
